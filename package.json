{
  "name": "jean-s-resume",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"vite\" \"npx tailwindcss -i ./src/App.css -o ./dist/main.css --watch\"",
